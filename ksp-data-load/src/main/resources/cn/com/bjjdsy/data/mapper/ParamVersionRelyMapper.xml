  <select id="selectByPrimaryKey" parameterType="java.lang.Short" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from PARAM_VERSION_RELY
    where ID = #{id,jdbcType=DECIMAL}
