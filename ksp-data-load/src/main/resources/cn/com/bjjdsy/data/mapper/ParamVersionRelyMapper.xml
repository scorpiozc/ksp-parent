  <select id="selectByVersionCode" parameterType="java.lang.String" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from PARAM_VERSION_RELY
    where VERSION_CODE = #{versionCode,jdbcType=VARCHAR}
  </select>
